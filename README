  /usr/lib/apache2/mod_clamav.la
  /usr/lib/apache2/mod_clamav.so
  /usr/share/doc/Cygwin/apache2-mod_clamav-0.21.README
  /usr/share/doc/apache2-mod_clamav-0.21/AUTHORS
